#include <bits/stdc++.h>
using namespace std;

void solution(void) {
  int T = 0, files = 0;
  std::cin >> T;
  for (int test_cases = 0; test_cases < T; test_cases++) {
    int n = 0, m = 0, A = 0, B = 0;
    std::cin >> n >> m >> A >> B;
    std:cout << n << " " << m << " " << A << " " << B << endl;
  }

  return;
}
